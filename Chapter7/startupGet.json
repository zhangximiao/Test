[
  {
  "description":"模拟一个没有参数的get请求",
  "request":{
    "uri":"/getdemo",
    "method":"get"
  },
  "response":{
    "text":"这是一个没有参数的get的请求",
    "headers":{
      "Content-Type":"text/html;charset=gbk"
    }
  }
},
  {
    "description":"这是我们模拟带参数的例子",
    "request":{
      "uri":"/getwithparam",
      "method":"get",
      "queries": {
       "name":"huhansan",
        "age": "18"
      }
    },
    "response":{
      "text":"我胡汉三有回来了！",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  }

]